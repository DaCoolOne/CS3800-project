

import os "os"

function main() {
    set int_i 1

    (os.print "FizzBuzz Program")

    for int_i (<= int_i 100) (++ int_i) {
        if (= (% int_i 15) 0) {
            (os.printTaggedNum "FizzBuzz, " int_i)
        }
        elif (= (% int_i 3) 0) {
            (os.printTaggedNum "Fizz, " int_i)
        }
        elif (= (% int_i 5) 0) {
            (os.printTaggedNum "Buzz, " int_i)
        }
        else {
            (os.printU int_i)
        }
    }
}