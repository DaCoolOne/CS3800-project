

import os "os"

function main() {

    (os.print "Factorial 1 to 8")
    
    set int_j 1
    for int_j (< int_j 9) (++ int_j) {
        set int_factorial 1

        set int_i 1
        for int_i (<= int_i int_j) (++ int_i) {
            set int_factorial (* int_factorial int_i)
        }

        (os.printU int_factorial)
    }
}

