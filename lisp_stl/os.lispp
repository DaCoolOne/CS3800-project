

function print(str_s) {
    set KERNEL.int_KERNELARG (addr (CastStrIntPtr str_s))

    # Invoke kernel to print
    (KERNEL.RAISE 6)
}

function printS(int_n) {
    set KERNEL.int_KERNELARG int_n

    # Invoke kernel to print
    (KERNEL.RAISE 7)
}

function printU(int_n) {
    set KERNEL.int_KERNELARG int_n

    # Invoke kernel to print
    (KERNEL.RAISE 8)
}

function printHex(int_n) {
    set KERNEL.int_KERNELARG int_n

    # Invoke kernel to print
    (KERNEL.RAISE 9)
}
